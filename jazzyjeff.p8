pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
death = {
	fr = 3,
	loop = false
}
death[1] = 1
death[2] = 18
death[3] = 19
death[4] = 20
death[5] = 21
death[6] = 22

walk = {
	fr = 4,
	loop = true
}
walk[1] = 1
walk[2] = 2
walk[3] = 3
walk[4] = 4

coinspin = {
	fr = 3,
	loop = true
}
coinspin[1] = 33
coinspin[2] = 34
coinspin[3] = 35
coinspin[4] = 36
coinspin[5] = 37
coinspin[6] = 38
coinspin[7] = 39
coinspin[8] = 40

player = {
	x = 20,
	y = 20,
	sprite = 1,
	speed = .5,
	dead = false,
	score = 0
}

g = {
	frame = 0,
	score = 0
}

ball = {
 sprite = 17,
 x = 44,
 y = 64,
 xspeed = 2,
 yspeed = 3
}

sounds = {
	death = 30,
	coin = 31
}

coin = {
	sprite = 33,
	x = 100,
	y = 100
}

music(0)

player_counter = 1
function animate_player(anim)
 player.moving=true
	if g.frame % anim.fr  == 0 then
		player.sprite = anim[player_counter]
		player_counter += 1
		if anim.loop then
			if (player_counter > #anim) player_counter = 1
		else
			if (player_counter > #anim) player_counter = #anim
		end
	end
end

coin_counter = 1
function animate_coin(anim)
	if g.frame % anim.fr  == 0 then
		coin.sprite = anim[coin_counter]
		coin_counter += 1
		if anim.loop then
			if (coin_counter > #anim) coin_counter = 1
		else
			if (coin_counter > #anim) coin_counter = #anim
		end
	end
end

function move_player()
 player.moving = false
 if player.dead then
   animate_player(death)
 end
 if (player.dead) return
 if btn(0) or btn(1) or btn(2) or btn(3) then
	animate_player(walk)
 end
 if btn(0) then
 	player.x -= player.speed
 end
 if btn(1) then
 	player.x += player.speed
 end
 if btn(2) then
 	player.y -= player.speed
 end
 if btn(3) then
 	player.y += player.speed
 end
 if player.x > 120 then
		player.x = 120
 end
 if player.x < 0 then
		player.x = 0
 end
 if player.y > 120 then
		player.y = 120
 end
 if player.y < 0 then
		player.y = 0
 end
 if not player.moving then
 	player.sprite = 1
 end
end

function move_ball()
	ball.x += ball.xspeed
	ball.y += ball.yspeed
end

function bounce_ball()
	if ball.x < 0 then
		ball.xspeed = -ball.xspeed
	end
	if ball.y < 0 then
		ball.yspeed = -ball.yspeed
	end
	if ball.x > 120 then
		ball.xspeed = -ball.xspeed
	end
	if ball.y > 120 then
		ball.yspeed = -ball.yspeed
	end
end

function collide_ball()
  if (player.dead) return
	if ball.x + 8 > player.x and ball.x < player.x + 8 and 
		 ball.y + 8 > player.y and ball.y < player.y + 8 then
		player.dead = true
		music(-1)
		sfx(sounds.death)
	end
end

function collect_coin()
		if coin.x + 8 > player.x and coin.x < player.x + 8 and 
		 coin.y + 8 > player.y and coin.y < player.y + 8 then
		 	player.score += 15
			coin.x = rnd(114) +8
			coin.y = rnd(114) +8
			sfx(sounds.coin)
		end
end

function _update()
  g.frame += 1
	animate_coin(coinspin)
  move_player()
	move_ball()
	bounce_ball()
	collide_ball()
	collect_coin()
end

function _draw()
 cls()
 map(0,0,0,0,16,16)
 print(player.score, 0, 0, 7)
 spr(player.sprite, player.x, player.y)
 spr(coin.sprite, coin.x, coin.y)
 spr(ball.sprite,ball.x, ball.y)
end 
